<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="coffee.flavors.unbit">
    <application
        android:name="coffee.flavors.unbit.core.UnbitApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppLightMtrlTheme"
        tools:ignore="AllowBackup">
        <activity android:name="coffee.flavors.unbit.view.screens.activities.ViewCreation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:name="coffee.flavors.unbit.view.screens.activities.ViewHobbies">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:name="coffee.flavors.unbit.view.screens.activities.ViewMembers">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="coffee.flavors.unbit.view.screens.activities.ViewSettings">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
